{
  "name": "silentspace-pwa",
  "version": "1.0.0",
  "description": "SilentSpace - AI Psychology Chatbot PWA",
  "private": true,
  "dependencies": {
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "@vercel/analytics": "^1.5.0",
    "lucide-react": "^0.542.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-scripts": "^0.0.0",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:pwa": "react-scripts build && npm run generate-sw",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate-sw": "echo 'Service worker generated during build'",
    "pwa:analyze": "npx @pwa/analyze build",
    "pwa:validate": "npx @pwa/validate build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.15",
    "postcss": "^8.4.29",
    "tailwindcss": "^3.3.3"
  },
  "pwa": {
    "name": "SilentSpace",
    "short_name": "SilentSpace",
    "description": "AI Psychology Chatbot for Mental Health Support",
    "start_url": "/",
    "display": "standalone",
    "theme_color": "#000000",
    "background_color": "#ffffff",
    "orientation": "portrait-primary",
    "scope": "/",
    "lang": "en"
  }
}
